<%= form_with model: equipe, class:"col-10" do |form| %>
    <div>
        <%= form.label :nome, class:"form-label mt-2" %><br>
        <%= form.text_field :nome, class:"form-control" %>
        <% equipe.errors.full_messages_for(:nome).each do |message| %>
            <div><%= message %></div>
        <% end %>
    </div>

    <div class="mt-3">
        <%= form.submit %>
    </div>
<% end %>